<section>

    <table class="table table-bordered table-striped text-center mt-4">
        <thead>
           <tr class="bg-black text-white">
            <th scope="col">Id</th>
                <th scope="col">Peso</th>
                <th scope="col">Color Musculo</th>
                <th scope="col">Calidad del marmoleado</th>
                <th scope="col">Temperatura</th>
                <th scope="col">Frecuencia cardiaca</th>
                <th scope="col">Frecuencia resporatoria</th>
                <th scope="col">Frecuencia Sanguinea</th>
           </tr>
        </thead>
        <tbody>
           <% results.forEach((ganado)=>{%>
            <tr>
                <td><%= ganado.idGanado %></td>
                <td><%= ganado.Peso%></td>
                <td><%= ganado.colorMusculo %></td>
                <td><%= ganado.calidad%></td>
                <td><%= ganado.temp %></td>
                <td><%= ganado.freCard %></td>
                <td><%= ganado.freRes %></td>
                <td><%= ganado.fresSan %></td>
            </tr>
                <%})%>
        </tbody>
    </table>
</section>
