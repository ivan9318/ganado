  <section>
    <main>
      <div id="carouselExampleControls" class="carousel slide" data-bs-ride="carousel">
        <div class="carousel-inner">
          <div class="carousel-item active">
            <img src="https://external-content.duckduckgo.com/iu/?u=https%3A%2F%2Fpanoramaagrario.com%2Fwp-content%2Fuploads%2F2014%2F09%2FCAMPOG1-scaled.jpg&f=1&nofb=1" class="d-block w-100 " alt="..." style="height: 500px;">
          </div>
          <div class="carousel-item">
            <img src="https://external-content.duckduckgo.com/iu/?u=https%3A%2F%2Fi1.wp.com%2Frioja24.com.ar%2Fwp-content%2Fuploads%2F2020%2F10%2FGanado-Bovino.jpg&f=1&nofb=1" class="d-block w-100" alt="..." style="height: 500px;">
          </div>
          <div class="carousel-item">
            <img src="https://external-content.duckduckgo.com/iu/?u=https%3A%2F%2Fstorage.contextoganadero.com%2Fs3fs-public%2Finternacional%2Ffield_image%2F2019-11%2Fganado_mexico_usa.jpeg&f=1&nofb=1" class="d-block w-100 " alt="..." style="height: 500px;">
          </div>
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="next">           <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="visually-hidden">Next</span>
        </button>
      </div>
    </main>





    <table id="ganado" class="table table-bordered table-striped text-center mt-4">
      <thead>
        <tr class="bg-black text-white">
          <th scope="col">Id</th>
          <th scope="col">Peso</th>
          <th scope="col">Color Musculo</th>
          <th scope="col">Calidad del marmoleado</th>
          <th scope="col">Temperatura</th>
          <th scope="col">Frecuencia cardiaca</th>
          <th scope="col">Frecuencia resporatoria</th>
          <th scope="col">Frecuencia Sanguinea</th>
        </tr>
      </thead>
      <tbody>
        <% results.forEach((ganado)=>{%>
          <tr>
            <td>
              <%= ganado.idGanado %>
            </td>
            <td>
              <%= ganado.Peso%>
            </td>
            <td>
              <%= ganado.colorMusculo %>
            </td>
            <td>
              <%= ganado.calidad%>
            </td>
            <td>
              <%= ganado.temp %>
            </td>
            <td>
              <%= ganado.freCard %>
            </td>
            <td>
              <%= ganado.freRes %>
            </td>
            <td>
              <%= ganado.fresSan %>
            </td>
          </tr>
          <%})%>
      </tbody>
    </table>
  </section>
